{
  "name": "AiHat-rag-poc-server",
  "version": "1.0.0",
  "type": "module",
  "description": "A proof of concept built to illustrate how RAG can be used",
  "main": "src/app.js",
  "repository": "https://github.com/vgflutter/AiHat-rag-poc-server.git",
  "author": "Vincenzo Giacchina <giacchina.vincenzo@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "start": "pm2 start ecosystem.config.json --no-daemon",
    "dev": "cross-env NODE_ENV=development nodemon src/server.js",
    "test": "jest -i --colors --verbose --detectOpenHandles",
    "start:chromadb": "docker run -d -p 8000:8000 -v $(pwd)/src/rag/chroma_db:/data --name chromadb chromadb/chroma:latest",
    "stop:chromadb": "docker stop chromadb && docker rm chromadb",
    "index-docs": "node indexDocs.js",
    "test:watch": "jest -i --watchAll",
    "coverage": "jest -i --coverage",
    "coverage:coveralls": "jest -i --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js",
    "prepare": "husky install"
  },
  "keywords": [
    "node",
    "node.js",
    "boilerplate",
    "generator",
    "express",
    "rest",
    "api",
    "mongodb",
    "mongoose",
    "es6",
    "es7",
    "es8",
    "es9",
    "jest",
    "travis",
    "docker",
    "passport",
    "joi",
    "eslint",
    "prettier"
  ],
  "dependencies": {
    "@google-cloud/speech": "^6.0.2",
    "@google-cloud/text-to-speech": "^5.0.1",
    "@google/generative-ai": "^0.22.0",
    "@langchain/community": "^0.3.42",
    "@langchain/core": "^0.3.55",
    "@langchain/openai": "^0.5.10",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^7.0.0",
    "dotenv": "^16.5.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.0",
    "express-rate-limit": "^5.0.0",
    "helmet": "^4.1.0",
    "http-status": "^1.4.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "langsmith": "^0.3.11",
    "moment": "^2.24.0",
    "mongoose": "^5.7.7",
    "morgan": "^1.9.1",
    "multer": "^1.4.5-lts.1",
    "multipart": "^0.1.5",
    "nodemailer": "^6.3.1",
    "openai": "^4.12.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdf-table-extractor": "^1.0.3",
    "pdf2json": "^3.1.4",
    "pdf2pic": "^3.1.3",
    "pdfjs-dist": "^4.8.69",
    "pm2": "^5.1.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^2.5.0",
    "swagger-jsdoc": "^6.0.8",
    "swagger-ui-express": "^4.1.6",
    "tesseract.js": "^5.1.1",
    "util": "^0.12.5",
    "validator": "^13.0.0",
    "whisper-node": "^0.2.12",
    "whisper-nodejs": "^1.0.0",
    "winston": "^3.2.1",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "chai": "^4.3.10",
    "coveralls": "^3.0.7",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-security": "^1.4.0",
    "faker": "^5.1.0",
    "husky": "7.0.4",
    "jest": "^26.0.1",
    "lint-staged": "^11.0.0",
    "mocha": "^10.2.0",
    "node-mocks-http": "^1.8.0",
    "nodemon": "^2.0.0",
    "prettier": "^2.0.5",
    "supertest": "^6.0.1"
  }
}
